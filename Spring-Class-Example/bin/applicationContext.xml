<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- Define your beans here -->
    
    
    
    <context:property-placeholder location="classpath:abc.properties"/>

  
  
  <bean id="myFortune" class="com.springDemo.HappyFortuneService"></bean>
  
  
  <bean id="myCoach" class="com.springDemo.BaseballCoach">
  
  <constructor-arg ref="myFortune"/>
  
  </bean>
  
    <bean id="myCricketCoach" class="com.springDemo.CricketCoach">
  
  <property name="fortuneService" ref="myFortune"/>
    <property name="team" value="${name}"/>
  
  </bean>
  
    
    
</beans>





